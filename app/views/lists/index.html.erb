<div class="list-wrap">
<% @lists.each do |list| %>
  <div class="list-box">
    <% if list.image.attached? %>
    <%= image_tag list.image, size: "200x200" %>
    <% else %>
    <%= image_tag 'no_image', size: "200x200" %>
    <% end %>
    <divã€€class="list-comment">
      <sapn class="list-body"><%= list.body %></sapn>
      <sapn class="list-title"><%= link_to list.title, list_path(list.id) %></sapn>
    </div>
  </div>
<% end %>
</div>


